{
  "name": "heroes-angular-serverless",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "node:build-ts": "tsc -p server",
    "node:watch-ts": "tsc -w -p server",
    "node:lint": "npm run node:build-ts && eslint \"server/**/*.ts\" --quiet --fix",
    "node:debug": "WWW=./dist/heroes-angular nodemon -r dotenv/config --inspect=9229 server/dist/index.js",
    "node:start": "WWW=./dist/heroes-angular node -r dotenv/config server/dist/index.js",
    "node-ng:build": "concurrently \"npm run node:build-ts\" \"npm run build\"",
    "node-ng:debug": "concurrently \"npm run node:debug\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@azure/cosmos": "^3.5.4",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ngrx/data": "^8.6.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "angular-font-awesome": "^3.1.2",
    "body-parser": "^1.19.0",
    "bulma": "^0.7.5",
    "core-js": "^2.6.11",
    "express": "^4.17.3",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/body-parser": "^1.17.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "codelyzer": "^5.2.1",
    "concurrently": "^4.1.2",
    "dotenv": "^7.0.0",
    "eslint": "^6.8.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.4",
    "protractor": "^5.4.3",
